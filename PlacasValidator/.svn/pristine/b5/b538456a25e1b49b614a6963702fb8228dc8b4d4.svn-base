<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/white" >

    <ImageView
        android:id="@+id/image_licenseStatus"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:contentDescription="@string/title_app"
        android:src="@drawable/no"
        android:visibility="invisible" />
    
   <ImageView
        android:id="@+id/image_title"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:contentDescription="@string/title_app"
        android:src="@drawable/neology_tb2" />

    
      

    <TextView
        android:id="@+id/label_tmpBalance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="10dp"
        android:fontFamily="sans-serif"
        android:text="@string/tmp_balance"
        android:textSize="60sp"
        android:textStyle="bold" />
    
    <ImageView
        android:id="@+id/color_placa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:layout_toRightOf="@id/label_tmpBalance"
        android:contentDescription="@string/title_app"
        android:src="@drawable/color_0"
        android:visibility="invisible"
         />
    
    
    


	<TextView 
	    android:id="@+id/label_dirterritorial"
	    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/label_tmpBalance"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:fontFamily="sans-serif-light"
        android:textStyle="italic"
        android:text="@string/dirterritorial_label"
        
	    />
	
	<TextView 
	    android:id="@+id/dirterritorial"
	    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/label_dirterritorial"
        android:layout_marginLeft="10dp"
        android:fontFamily="sans-serif"
        android:textSize="25sp"
        android:textStyle="bold"
        android:text=""
	    />
	
	<TextView 
	    android:id="@+id/label_dirpolicial"
	    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/dirterritorial"
        android:layout_marginLeft="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:fontFamily="sans-serif-light"
        android:textStyle="italic"
        android:text="@string/dirpolicial_label"
        
	    />
	
	<TextView 
	    android:id="@+id/dirpolicial"
	    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/label_dirpolicial"
        android:layout_marginLeft="10dp"
        android:fontFamily="sans-serif"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text=""
	    />

    <!--
    <Button
        android:id="@+id/button_recharge"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/dirpolicial"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp"
        android:drawableStart="@drawable/busqueda"
        android:fontFamily="sans-serif"
        android:height="130dp"
        android:onClick="openLicenseData"
        android:text="@string/recharge_label"
        android:textSize="36sp"
        android:textStyle="normal"
        android:width="300dp"
        />

     -->
    
    <!-- Cuando se tiene habilitado para grabado -->

	 
    <Button
        android:id="@+id/button_recharge"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/dirpolicial"
        android:fontFamily="sans-serif"
        android:height="50dp"
        android:onClick="openLicenseData"
        android:text="@string/recharge_label"
        android:textSize="20sp"
        android:textStyle="normal"
        android:width="300dp" />

    <Button
        android:id="@+id/button_readTagNFC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_below="@id/button_recharge"
        android:layout_centerHorizontal="true"
        android:fontFamily="sans-serif"
        android:height="50dp"
        android:onClick="openReadNFC"
        android:text="@string/readTagNFC_label"
        android:textSize="20sp"
        android:textStyle="normal"
        android:width="300dp" />
        
 
    

</RelativeLayout>